@attribute [StreamRendering]
@rendermode InteractiveServer

<style>
    .header-menu {
        padding-top: 20px;
        color: white !important;
        margin-bottom: 10px !important;
    }

        .header-menu .titulo {
            font-size: 1.4rem;
        }

        .header-menu .subtitulo {
            font-size: 1.2rem;
        }

        .header-menu .icon {
